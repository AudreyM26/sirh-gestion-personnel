<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<display-name>sgp</display-name>
	<servlet>
		<servlet-name>jsp</servlet-name>
		<servlet-class>org.apache.jasper.servlet.JspServlet</servlet-class>
		<init-param>
			<param-name>compilerSourceVM</param-name>
			<param-value>1.8</param-value>
		</init-param>
		<init-param>
			<param-name>compilerTargetVM</param-name>
			<param-value>1.8</param-value>
		</init-param>
	</servlet>
	<servlet>
		<servlet-name>ListerCollaborateursController</servlet-name>
		<servlet-class>dev.sgp.web.ListerCollaborateursController
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ListerCollaborateursController</servlet-name>
		<url-pattern>/collaborateurs/lister</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>EditerCollaborateurController</servlet-name>
		<servlet-class>dev.sgp.web.EditerCollaborateurController
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EditerCollaborateurController</servlet-name>
		<url-pattern>/collaborateurs/editer</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CreerCollaborateurController</servlet-name>
		<servlet-class>dev.sgp.web.CreerCollaborateurController
		</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CreerCollaborateurController</servlet-name>
		<url-pattern>/collaborateurs/creer</url-pattern>
	</servlet-mapping>

</web-app>